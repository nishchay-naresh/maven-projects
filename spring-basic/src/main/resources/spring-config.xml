<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id="e1" class="com.nish.springannotation.model.Engine">
        <property name="horsePower" value="150"/>
    </bean>

    <bean id="bike" class="com.nish.springannotation.model.Bike"/>

    <bean id="e2" class="com.nish.springannotation.model.Engine">
        <qualifier value="bikeEngine"/>
        <property name="horsePower" value="220"/>
    </bean>


</beans>